{
  "name": "mity-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc && npm run copy-seed",
    "copy-seed": "cp -r src/seed/*.json dist/seed/",
    "watch": "npx tsc -w",
    "watch-seed": "npx npm-run-all --parallel watch copy-seed-watch",
    "copy-seed-watch": "npx nodemon --watch src/seed --ext json --exec \"npm run copy-seed\"",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "devstart": "npx nodemon dist/server.js",
    "dev": "npm run copy-seed && npx npm-run-all --parallel watch-seed devstart"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/eventsource": "^1.1.15",
    "@types/node": "^22.5.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.5",
    "eventsource": "^3.0.5",
    "hr-sdk": "^0.1.1",
    "lokijs": "^1.5.12",
    "node-cron": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "socket.io-client": "^4.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/lokijs": "^1.5.14",
    "@types/node-cron": "^3.0.11",
    "@types/ssh2-sftp-client": "^9.0.4",
    "typescript": "^5.7.2"
  }
}
